{
  "name": "deadlineprogressbar",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "appName": "DeadLineProgressBar",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "electron .",
    "lint:prettier": "prettier --write \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/"
  },
  "author": "pealpool",
  "license": "ISC",
  "devDependencies": {
    "electron": "^19.0.4",
    "html-webpack-plugin": "^5.5.0",
    "postcss-html": "^1.4.1",
    "prettier": "^2.6.2",
    "stylelint": "^14.9.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-order": "^5.0.0"
  },
  "dependencies": {
    "@electron/remote": "^2.0.1",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "electron-dev-webpack-plugin": "^1.0.5",
    "electron-drag": "^2.0.0",
    "jquery": "^3.6.0",
    "mini-css-extract-plugin": "^2.4.5",
    "node-loader": "^2.0.0",
    "node-sass": "^7.0.1",
    "sass-loader": "^13.0.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "webpack-hot-middleware": "^2.25.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pealpool/DeadlineProgressBar.git"
  },
  "bugs": {
    "url": "https://github.com/pealpool/DeadlineProgressBar/issues"
  },
  "homepage": "https://github.com/pealpool/DeadlineProgressBar#readme",
  "build": {
    "asar": true,
    "productName": "DeadLineProgressBar",
    "appId": "com.DeadLineProgressBar",
    "copyright": "Copyright © pealpool",
    "directories": {
      "output": "pack"
    },
    "files": [
      "OutputApp/**"
    ],
    "mac": {
      "identity": "com.electron.pealpool",
      "target": [
        "dmg"
      ],
      "artifactName": "${productName}.${ext}",
      "icon": "img/ico128.ico"
    },
    "dmg": {
      "title": "${productName}",
      "artifactName": "${productName}.${ext}",
      "icon": "img/ico128.ico"
    },
    "win": {
      "legalTrademarks": "Copyright © pealpool",
      "publisherName": "electron",
      "requestedExecutionLevel": "highestAvailable",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32"
          ]
        }
      ],
      "artifactName": "${productName}.${ext}",
      "icon": "img/ico128.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "perMachine": true,
      "allowElevation": true,
      "artifactName": "${productName}-安装包-V${version}.${ext}",
      "runAfterFinish": true,
      "shortcutName": "Electron+vue+webpack-template"
    }
  }
}
